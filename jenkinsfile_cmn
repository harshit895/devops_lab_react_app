
node() {

  currentBuild.result = "SUCCESS"
  try {
        stage 'Checkout'
                checkout scm
    
        stage 'Health Check'
                sh '''
                      cd ${WORKSPACE}
                      chmod +x health.sh
                      ./health.sh
                   '''
        
        stage 'Deploy'
                sh '''
                    echo "Proceeded to Next Step"
                   '''
                    
  }
  catch(err) {
      currentBuild.result = "FAILURE"
      throw err
  }
}
