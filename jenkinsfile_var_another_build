node() {
 
  currentBuild.result = "SUCCESS"
  try {
        stage 'Checkout'
                checkout scm
    
        stage 'Trigger Branch Build'
                build job: 'test2', parameters: [[$class: 'StringParameterValue', name: 'APP_URL', value: systemname]]

 }
  catch(err) {
      currentBuild.result = "FAILURE"
      throw err
  }
}
