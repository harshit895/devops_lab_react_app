node() {
 
  currentBuild.result = "SUCCESS"
  try {
        stage 'Checkout'
                checkout scm
    
        stage 'Health Check'
                sh '''
                 export APP_URL='http://google.com'
                 build job: test2
                 '''
                    
  }
  catch(err) {
      currentBuild.result = "FAILURE"
      throw err
  }
}
