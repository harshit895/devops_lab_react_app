node() {

  currentBuild.result = "SUCCESS"
  try {
        stage 'Checkout'
                checkout scm
    
        stage 'Running Script'
                sh '''
                      cd ${WORKSPACE}
                      SCRIPT_PATH=awk -F "" 'NR==1{print}' params.txt
                      PARAM1=awk -F "" 'NR==2{print}' params.txt
                      PARAM2=awk -F "" 'NR==3{print}' params.txt
                      chmod +x $SCRIPT_PATH
                      $SCRIPT_PATH $PARAM1 $PARAM2
                   '''
                    
  }
  catch(err) {
      currentBuild.result = "FAILURE"
      throw err
  }
}
