node() {

  currentBuild.result = "SUCCESS"
  try {
        stage 'Checkout'
                checkout scm
    
        stage 'Running Script'
                sh '''
                      #echo ${WORKSPACE}
                      chmod +x ${WORKSPACE}/${SCRIPT_PATH}.sh
                      ${WORKSPACE}/${SCRIPT_PATH}.sh ${SCHEMA_NAME} ${CRON_EXPRESSION}
                   '''
                    
  }
  catch(err) {
      currentBuild.result = "FAILURE"
      throw err
  }
}
